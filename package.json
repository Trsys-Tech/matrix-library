{
  "name": "@trsys-tech/matrix-library",
  "description": "MatrixUI Library",
  "private": false,
  "version": "0.5.1",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.es.js"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*",
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:styles": "npx tailwindcss -i ./src/index.css -o ./dist/style.css --watch",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "sb": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Trsys-Tech/matrix-library.git"
  },
  "license": "MIT",
  "author": "Ako Veysinejad <a.veysinejad@gmail.com>",
  "keywords": [
    "matrix",
    "ui",
    "library",
    "react",
    "typescript"
  ],
  "homepage": "https://github.com/Trsys-Tech/matrix-library#readme",
  "bugs": {
    "url": "https://github.com/Trsys-Tech/matrix-library/issues"
  },
  "contributors": [
    "Ako Veysinejad <a.veysinejad@gmail.com>"
  ],
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.1",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-collapsible": "^1.1.1",
    "@radix-ui/react-context": "^1.1.1",
    "@radix-ui/react-context-menu": "^2.2.2",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-radio-group": "^1.2.1",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@radix-ui/react-visually-hidden": "^1.1.0",
    "@trsys-tech/matrix-icons": "^0.1.0",
    "ag-grid-community": "^33.3.0",
    "ag-grid-react": "^33.3.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "lucide-react": "^0.454.0",
    "react-day-picker": "^9.3.0",
    "tailwind-merge": "^2.5.4",
    "tailwind-variants": "^0.3.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.0.5",
    "vaul": "^1.1.1"
  },
  "peerDependencies": {
    "@hookform/resolvers": "^3.9.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.61.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.9.0",
    "@eslint/js": "^9.13.0",
    "@storybook/addon-essentials": "^8.4.1",
    "@storybook/addon-interactions": "^8.4.1",
    "@storybook/addon-onboarding": "^8.4.1",
    "@storybook/blocks": "^8.4.1",
    "@storybook/react": "^8.4.1",
    "@storybook/react-vite": "^8.4.1",
    "@storybook/test": "^8.4.1",
    "@types/node": "^22.8.4",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "ajv": "^8.17.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-storybook": "^0.10.1",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "3.3.3",
    "storybook": "^8.3.6",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
